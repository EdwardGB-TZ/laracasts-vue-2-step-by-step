{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "webpack": "export NODE_ENV=development && webpack --hide-modules",
        "webpack-watch": "export NODE_ENV=development && webpack --hide-modules --watch",
        "webpack-production": "export NODE_ENV=production && node_modules/.bin/webpack --hide-modules"
    },
    "devDependencies": {
        "axios": "^0.21.1",
        "laravel-mix": "^6.0.25",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "vue": "^2.6.14"
    },
    "dependencies": {}
}
